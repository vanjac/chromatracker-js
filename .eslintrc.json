{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 6
    },
    "rules": {
        "no-undef": "off",
        "no-unused-vars": "off",
        "no-var": "error",
        "semi": "error",
        "strict": ["error", "global"]
    }
}
